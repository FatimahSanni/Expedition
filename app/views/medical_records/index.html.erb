<p id="notice"><%= notice %></p>

<h1>Listing Medical Records</h1>

<table>
  <thead>
    <tr>
      <th>Complaints</th>
      <th>Pre lva</th>
      <th>Pre rva</th>
      <th>Diagnosis</th>
      <th>Eye</th>
      <th>Treatment</th>
      <th>Booked</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medical_records.each do |medical_record| %>
      <tr>
        <td><%= medical_record.complaints %></td>
        <td><%= medical_record.pre_lva %></td>
        <td><%= medical_record.pre_rva %></td>
        <td><%= medical_record.diagnosis %></td>
        <td><%= medical_record.eye %></td>
        <td><%= medical_record.treatment %></td>
        <td><%= medical_record.booked %></td>
        <td><%= link_to 'Show', medical_record %></td>
        <td><%= link_to 'Edit', edit_medical_record_path(medical_record) %></td>
        <td><%= link_to 'Destroy', medical_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medical record', new_medical_record_path %>
